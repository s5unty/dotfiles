HostkeyAlgorithms +ssh-rsa
PubkeyAcceptedKeyTypes +ssh-rsa

# Use Proxy for Git/GitHub
# https://gist.github.com/coin8086/7228b177221f6db913933021ac33bb92
Host github.com
  User git
  ProxyCommand nc -X connect -x localhost:1080 %h %p

Host sail
    Hostname du1ab.one
    User root

Host it
    Hostname wzf.trafpevcg.pbz
    User fhajhna
    ControlPath /var/tmp/.ssh-%h-%p-%r
    ControlMaster auto
    ControlPersist 168h

Host worktop
    Hostname 10.1.54.200
    ForwardX11 yes
    User wuan

